<mvc:View
  controllerName="chuntian.home.controller.App"
  xmlns="sap.m"
  xmlns:mvc="sap.ui.core.mvc"
  displayBlock="true">
  <App initialPage="Home">
    <pages>
      <Page title ="{i18n>homePageTitle}" id ="Home" 
            showNavButton="true" navButtonPress= "OnNav2Arial"
            navButtonTooltip ="{i18n>toArial}"
            navButtonText ="{i18n>Nav2Arial}"
            icon ="sap-icon://account">
        <content>
          <mvc:XMLView viewName="chuntian.home.view.HomePage.Panel1"/>
          <mvc:XMLView viewName="chuntian.home.view.HomePage.Panel2"/>
          <mvc:XMLView viewName="chuntian.home.view.HomePage.Panel3"/>
          <mvc:XMLView viewName="chuntian.home.view.HomePage.Panel4"/>
        </content>
      </Page>
      <Page title ="{i18n>arialPageTitle}" id ="Arial">
        <content>
          <Panel>
            <Text text="Hello World"/>
            <Link text ="Arial 简历" href="./ArialResume/ArialResumeIndex.html" />
            <Link text ="SAPUI5 Demo - Hello World!" href="./Chunge_UI5/HelloWorld/Helloworld.html"/>
            <Link text ="SAPUI5 Demo Apps" href="https://sapui5.netweaver.ondemand.com/sdk/#/demoapps"/>
            <Button text ="{i18n>clickMe}" press = "onShowHello"/>
            <Input value ="{/recipient/name}"
                   description ="Hello {/recipient/name}(JSONModel)"
                   valueLiveUpdate="true"
                   width="60%"/>
          </Panel>
        </content>
      </Page>
    </pages>
  </App>
</mvc:View>