{
	"_version": "1.8.0",
	"sap.app": {
		"id": "chuntian.PracticeHome",
		"type": "application",
		"i18n": "i18n/i18n.properties",
		"applicationVersion": {
			"version": "1.0.0"
		},
		"title": "{{appTitle}}",
		"description": "{{appDescription}}"
	},

	"sap.ui": {
		"technology": "UI5",
		"deviceTypes": {
			"desktop": true,
			"tablet": true,
			"phone": true
		}
	},

  "sap.ui5": {
    "rootView": {
      "viewName": "chuntian.PracticeHome.view.App",
      "type": "XML",
      "async": true,
      "id": "app"
    },
    "dependencies": {
      "minUI5Version": "1.30.0",
      "libs": {
        "sap.ui.core": {},
        "sap.m": {},
        "sap.ui.layout": {}
      }
    },
    "contentDensities": {
      "compact": true,
      "cozy": true
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "chuntian.PracticeHome.i18n.i18n"
        }
      },
      "practice": {
        "type": "sap.ui.model.json.JSONModel",
        "uri": "Practices.json"
      }
    },
    "resources": {
      "css": [
        {
          "uri": "css/style.css"
        }
      ]
    },
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "viewType": "XML",
        "viewPath": "chuntian.PracticeHome.view",
        "controlId": "app",
        "controlAggregation": "pages",
        "async": true
      },
      "routes": [
        {
          "pattern": "",
          "name": "PracticeHome",
          "target": "PracticeHome"
        },
        {
          "pattern": "helloworld",
          "name": "helloworld",
          "target": "helloworld"
        }
      ],
      "targets": {
        "PracticeHome": {
          "viewId": "PracticeHome",
          "viewName": "PracticeList"
        },
        "helloworld": {
          "viewId": "helloworld",
          "viewName": "Practice.HelloWorld"
        }
      }
    }
  }
}