{
  "_version": "1.8.0",
  "sap.app": {
    "id": "chuntian.home",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "applicationVersion": {
      "version": "1.0.0"
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "rootView": {
      "viewName": "chuntian.home.view.App",
      "type": "XML",
      "async": true,
      "id": "chuntian"
    },
    "dependencies": {
      "minUI5Version": "1.30",
      "libs": {
        "sap.m": {}
      }
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "chuntian.home.i18n.i18n"
        }
      },
      "practice": {
        "type": "sap.ui.model.json.JSONModel",
        "uri": "Practices.json"
      }
    },
    "css": [
      {
        "uri": "css/style.css"
      }
    ],
    "routing": {
      "config": {
        "routerClass": "sap.m.routing.Router",
        "viewType": "XML",
        "viewPath": "chuntian.home",
        "controlId": "chuntian",
        "controlAggregation": "pages",
        "async": true
      },
      "routes": [
        {
          "pattern": "",
          "name": "homepage",
          "target": "homepage"
        },
        {
          "pattern": "page1",
          "name": "page1",
          "target": "page1"
        },
        {
          "pattern": "project_summary",
          "name": "project_summary",
          "target": "project_summary"
        }
      ],
      "targets": {
        "homepage": {
          "viewID": "homepage",
          "viewName": "view.HomePage"
        },
        "page1": {
          "viewId": "page1",
          "viewName": "view.Page1"
        },
        "page2": {
          "viewId": "page2",
          "viewName": "view.Page2"
        },
        "page3": {
          "viewId": "page3",
          "viewName": "view.Page1"
        },
        "page4": {
          "viewId": "page4",
          "viewName": "view.Page2"
        },
        "project_summary": {
          "viewId": "projectSummary",
          "viewName": "Chunge_UI5.PracticsHome.view.PracticeList"
        }
      }
    }
  }
}