{
  "_version": "1.8.0",
  "sap.app": {
    "id": "chuntian.home",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "applicationVersion": {
      "version": "1.0.0"
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "rootView": {
      "viewName": "chuntian.home.view.App",
      "type": "XML",
      "async": true,
      "id": "chuntian"
    },
    "dependencies": {
      "minUI5Version": "1.30",
      "libs": {
        "sap.m": {}
      }
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "chuntian.home.i18n.i18n"
        }
      }
    },
    "css": [
      {
        "uri":  "css/style.css"
      }
    ]
  }
}