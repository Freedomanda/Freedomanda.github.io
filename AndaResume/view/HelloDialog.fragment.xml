<core:FragmentDefinition
   xmlns="sap.m"
   xmlns:core="sap.ui.core" >
  <Dialog
     id="helloDialog"
     title="{i18n>dialogTitle}">
    <content>
      <core:Icon
         src="sap-icon://bar-code"
         size="8rem"
         class="sapUiMediumMargin"/>
       <Image src = ".source.wechat.png"/>
    </content>
    <beginButton>
      <Button
         text="{i18n>dialogCloseButtonText}"
         press="onCloseDialog"/>
    </beginButton>
  </Dialog>
</core:FragmentDefinition>